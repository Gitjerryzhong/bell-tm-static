{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "importHelpers": true,
    "baseUrl": "./src",
    "lib": [
      "dom",
      "es6"
    ],
    "types": [
      "bootstrap",
      "jquery",
      "node"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useWebpackText": true
  }
}
